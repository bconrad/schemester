{
	"name": "My First Extension",
		"version": "1.0",
		"description": "The first extension that I made.",
		"browser_action": {
			"default_icon": "icon.png",
			"popup": "popup.html"
		},
		"permissions": [
			"tabs", "http://*/*"
			],
		"content_scripts": [
		{
			"matches": ["http://*/*"],
			"js": ["content.js"]
		}],
		"background_page": "background.html"

}
